    "author": "CertNode",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/srbryant86/certnode.git",
        "directory": "sdk/node"
    },
    "files": [
        "index.js",
        "index.d.ts",
        "README.md"
