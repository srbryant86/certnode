{
    "name": "@certnode/sdk",
    "version": "1.0.5",
    "description": "Minimal Node SDK for CertNode receipt verification",
    "main": "index.js",
    "types": "index.d.ts",
    "exports": {
        ".": {
            "require": "./index.js",
            "types": "./index.d.ts"
        }
    },
    "engines": {
        "node": ">=20.0.0"
    },
    "sideEffects": false,
    "publishConfig": {
        "access": "public"
    },
    "homepage": "https://github.com/srbryant86/certnode/tree/main/sdk/node",
