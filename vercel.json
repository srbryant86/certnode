{
  "version": 2,
  "builds": [
    {
      "src": "api/src/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "web/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/src/index.js"
    },
    {
      "src": "/validator",
      "dest": "/web/verify.html"
    },
    {
      "src": "/implementation",
      "dest": "/web/pricing.html"
    },
    {
      "src": "/specification",
      "dest": "/web/openapi.html"
    },
    {
      "src": "/verify",
      "dest": "/web/verify.html"
    },
    {
      "src": "/pricing",
      "dest": "/web/pricing.html"
    },
    {
      "src": "/openapi",
      "dest": "/web/openapi.html"
    },
    {
      "src": "/trust",
      "dest": "/web/trust.html"
    },
    {
      "src": "/",
      "dest": "/web/index.html"
    },
    {
      "src": "/(.*\\.(css|js|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot))",
      "dest": "/web/$1"
    }
  ]
}